 <!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.app.domain.License" table="license_o2ofk">
		<id name="lid">
			<generator class="sequence">
				<param name="sequence_name">license_seq</param>
			</generator>
		</id>
		<property name="type"/>
		<property name="validFrom" type="date"/>
		<property name="validTo" type="date"/>
		<many-to-one name="licenseHolder" class="com.app.domain.Person"
																				 column="license_holder"
																				 unique="true" 
																				 not-null="true"
																				 cascade="all-delete-orphan"
																				 lazy="proxy"
																				  />
	</class>
</hibernate-mapping>